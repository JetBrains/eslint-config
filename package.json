{
  "name": "@jetbrains/eslint-config",
  "version": "4.0.2",
  "description": "JetBrains config for ESLint",
  "author": "JetBrains",
  "license": "Apache-2.0",
  "main": "index.js",
  "files": [
    "*.js",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JetBrains/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "jetbrains"
  ],
  "dependencies": {
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-angular": "3.3.0",
    "eslint-plugin-bdd": "2.1.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-react": "7.10.0"
  },
  "devDependencies": {
    "ci-publish": "^1.3.1",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "standard-version": "^4.4.0"
  },
  "peerDependencies": {
    "eslint": ">=4.10.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "precommit": "lint-staged",
    "commit": "git-cz",
    "lint": "eslint .",
    "release:ci": "npm run lint && standard-version && git push --follow-tags origin master && ci-publish"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
